<h1>Dashboard</h1>



<%= form_with do |f| %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name, autocomplete: "name" %>
  </div>


  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  
  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>
